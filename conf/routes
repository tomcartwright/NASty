# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /drive.scala.html           controllers.Application.drive
GET     /settings.scala.html        controllers.Application.settings
GET     /trash.scala.html           controllers.Application.trash
GET     /starred.scala.html         controllers.Application.starred


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

    